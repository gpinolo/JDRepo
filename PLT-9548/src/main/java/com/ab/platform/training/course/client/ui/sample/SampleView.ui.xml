<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:date-mys="urn:import:com.ab.oneleo.uis.gwt.client.widget.date.monthyearselector">

	<ui:with field='SampleMessages' type='com.ab.platform.training.course.client.i18n.Messages' />

	<g:HTMLPanel>
        <g:Label ui:field="completionLabel1" />
        <g:TextBox text="{SampleMessages.sampleText}" />
        <date-mys:DateBox ui:field="dateBox" format="dd/MM/yyyy"/>
        <b:Button ui:field="dosomething" text="{SampleMessages.dosomething}" />
	</g:HTMLPanel>

</ui:UiBinder>
